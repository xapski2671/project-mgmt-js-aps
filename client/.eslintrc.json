{
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": ["warn", "double"],
    "semi": ["warn", "never"],
    "indent": ["warn", 2],
    "no-multi-spaces": ["warn"],
    "no-console": "off",
    "object-curly-spacing": ["warn", "always"],
    "react-hooks/exhaustive-deps": "off",
    "space-infix-ops": ["warn", { "int32Hint": false }]
  }
}
